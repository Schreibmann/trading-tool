{
  "name": "Trading-tool",
  "version": "1.3.0",
  "description": "A tool for EXMO exchange. Based on exmo api",
  "main": "main.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --mode production --config webpack.prod.config.js",
    "dev": "webpack-dev-server --mode development --open --hot --config webpack.dev.config.js",
    "lint": "eslint ./app/**/**.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "webpack:dev": "webpack --colors",
    "webpack:prod": "webpack --config webpack.production.config.js --colors",
    "heroku-postbuild": "npm run production"
  },
  "author": "Schreibmann",
  "homepage": "http://trading-tool.pro",
  "license": "MIT",
  "jest": {
    "setupFiles": [
      "<rootDir>/config/jest/shim.js"
    ],
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/config/jest/assetsTransformer.js"
    },
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "transform": {
      "\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "autoprefixer": "^9.3.1",
    "crypto-js": "^3.1.9-1",
    "react": "16.5.2",
    "react-awesome-modal": "^2.0.5",
    "react-dom": "16.5.2",
    "react-hot-loader": "4.3.11",
    "react-redux": "^5.0.7",
    "react-router-dom": "4.3.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.1",
    "@babel/core": "^7.1.1",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-stage-2": "7.0.0",
    "@babel/register": "7.0.0",
    "ajv": "^6.12.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "8.0.3",
    "babel-preset-react-hmre": "1.1.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "4.5.4",
    "css-hot-loader": "1.4.2",
    "css-loader": "1.0.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "extract-loader": "^3.0.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.1.2",
    "jest": "23.6.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "4.9.4",
    "open-browser-webpack-plugin": "0.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.5.2",
    "regenerator-runtime": "0.12.1",
    "rimraf": "2.6.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "1.3.0",
    "url-loader": "1.1.2",
    "webpack": "^4.16.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-merge": "^4.1.3"
  }
}
